{
  "name": "construction-catalog-spa",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && node scripts/build.mjs",
    "typecheck": "tsc --noEmit",
    "start": "node dist/server.js",
    "dev": "npm run build && node dist/server.js",
    "test": "jest",
    "lint": "eslint \"{src,tests}/**/*.ts\" server.ts",
    "format": "prettier --write \"{src,tests}/**/*.{ts,js,json,md}\" index.html styles.css README.md server.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "express-rate-limit": "^7.0.0",
    "localforage": "^1.10.0",
    "lz-string": "^1.5.0",
    "node-fetch": "^3.3.2",
    "sortablejs": "^1.15.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
